<UserControl x:Class="ExamenSegundoParcial.ControlContacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock>Correo:</TextBlock>
        <TextBox x:Name="txtCorreo"></TextBox>

        <TextBlock>Teléfono:</TextBlock>
        <TextBox x:Name="txtTelefono"></TextBox>

        <TextBlock>Celular:</TextBlock>
        <TextBox x:Name="txtCelular"></TextBox>

    </StackPanel>
</UserControl>
