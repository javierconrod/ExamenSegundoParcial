<UserControl x:Class="ExamenSegundoParcial.ControlInformacionPersonal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock>Nombre:</TextBlock>
        <TextBox x:Name="txtNombre"></TextBox>

        <TextBlock>Apellido Paterno</TextBlock>
        <TextBox x:Name="txtApellidoPaterno"></TextBox>

        <TextBlock>Apellido Materno</TextBlock>
        <TextBox x:Name="txtApellidoMaterno"></TextBox>

        <TextBlock>Edad</TextBlock>
        <TextBox x:Name="txtEdad"></TextBox>
    </StackPanel>
</UserControl>
